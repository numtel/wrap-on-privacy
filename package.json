{
  "name": "wrap-on-privacy",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "todo": "bash -c \"grep -rn 'TODO' --exclude-dir={node_modules,build,cache,lib,out} --exclude={*.min*,*.swp,package.json,solc} *\"",
    "dev": "vite",
    "build:frontend": "vite build",
    "test:libraries": "npx --node-options='--max-old-space-size=16240' mocha -t 500000",
    "test:contracts": "forge test",
    "test": "mocha -t 50000 && forge test",
    "compile:circuit": "circomkit compile privacytoken",
    "compile:mint-circuit": "circomkit compile privatemint",
    "deploy:circuit": "circuitscan deploy:circom -b -p groth16 -v v2.2.0 -s 0.7.5 main/privacytoken.circom",
    "deploy:mint-circuit": "circuitscan deploy:circom -b -p groth16 -v v2.2.0 -s 0.7.5 main/privatemint.circom",
    "deploy:contract": "VERIFIER_ADDRESS=0x60d9bbec388cb5804c8ede5510b81f783476ee38 MINT_VERIFIER_ADDRESS=0x31de5d6e9675479a5f30c6389bd43fc9b6f47fe9 forge script script/PrivateToken.s.sol:Deploy --rpc-url https://rpc.ankr.com/eth_sepolia  --broadcast --verify -vvvv",
    "deploy:registry-contract": "forge script script/KeyRegistry.s.sol:Deploy --rpc-url https://rpc.ankr.com/eth_sepolia  --broadcast --verify -vvvv",
    "deploy:frontend": "rm -rf dist/ && vite build && aws s3 sync dist/ s3://wrap.clonk.me/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@heroicons/react": "^2.1.3",
    "@rainbow-me/rainbowkit": "^2.1.4",
    "@tanstack/react-query": "^5.29.0",
    "@zk-kit/circuits": "0.2.3",
    "@zk-kit/imt": "2.0.0-beta",
    "circomlib": "2.0.5",
    "ffjavascript": "0.2.62",
    "ntru-circom": "0.0.10",
    "poseidon-lite": "0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.4.1",
    "snarkjs": "0.7.5",
    "viem": "2.21.55",
    "wagmi": "^2.12.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "circomkit": "0.3.1",
    "circuitscan": "0.1.3",
    "mocha": "10.8.2",
    "postcss": "^8.4.38",
    "rollup-plugin-copy": "^3.5.0",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.7"
  }
}
